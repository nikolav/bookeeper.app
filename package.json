  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn run watch:css\" \"vite --host\" ",
    "build": "vite build",
    "preview": "vite preview",
    "sass:build": "sass ./src/global.scss ./src/styles/global.css ",
    "sass:watch": "sass ./src/global.scss ./src/styles/global.css --watch",
    "build:css": "tailwindcss -i ./src/tailwind.css -o ./src/styles/build.css",
    "watch:css": "tailwindcss -i ./src/tailwind.css -o ./src/styles/build.css --watch"
  },
